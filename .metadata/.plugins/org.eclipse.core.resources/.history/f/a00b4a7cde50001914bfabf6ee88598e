package com.example.demo.mapper;

import java.sql.ResultSet;
import java.sql.SQLException;

import org.springframework.jdbc.core.RowMapper;

import mx.ihsa.springmvc.model.Login;

import java.sql.ResultSet;
import java.sql.SQLException;

import org.springframework.jdbc.core.RowMapper;

import mx.ihsa.springmvc.model.Login;

public class LoginMapper implements RowMapper<Login> {

	@Override
	public Usario mapRow(ResultSet rs, int rowNum) throws SQLException {
		Usuario usuario = new Usuario();
		login.setUsuario(rs.getString("id"));
		login.setPassword(rs.getString("nombre"));
		login.setPassword(rs.getString("edad"));
	return login;

	}
	
}
